{
  "name": "@join-com/jest-matchers",
  "version": "1.0.6",
  "description": "Set of custom jest matchers",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types/index.d.ts"
  ],
  "scripts": {
    "test": "jest",
    "lint": "tslint -p .",
    "prepublish": "tsc"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "yarn prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/join-com/jest-matchers"
  },
  "keywords": [
    "jest",
    "matchers"
  ],
  "author": "JOIN team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/join-com/jest-matchers/issues"
  },
  "homepage": "https://github.com/join-com/jest-matchers#readme",
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/jest": "^28.1.3",
    "@types/jest-diff": "^24.3.0",
    "@types/jest-matcher-utils": "^25.1.0",
    "@types/node": "^18.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "expect": "^24.8.0",
    "jest-diff": "24.8.0",
    "jest-matcher-utils": "24.8.0"
  }
}
