{
  "name": "@join-com/jest-matchers",
  "version": "1.0.1",
  "description": "Set of custom jest matchers",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types/index.d.ts"
  ],
  "scripts": {
    "test": "jest",
    "lint": "tslint -p .",
    "prepublish": "tsc"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "yarn prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/join-com/jest-matchers"
  },
  "keywords": [
    "jest",
    "matchers"
  ],
  "author": "JOIN team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/join-com/jest-matchers/issues"
  },
  "homepage": "https://github.com/join-com/jest-matchers#readme",
  "devDependencies": {
    "@types/expect": "^1.20.4",
    "@types/jest": "^24.0.13",
    "@types/jest-diff": "^20.0.1",
    "@types/jest-matcher-utils": "^21.0.2",
    "@types/node": "^12.0.2",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "expect": "^24.8.0",
    "jest-diff": "24.8.0",
    "jest-matcher-utils": "24.8.0"
  }
}
